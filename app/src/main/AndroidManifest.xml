<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bohan.finaldemo">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClientLoginActivity">
        </activity>
        <activity android:name=".CheckActivity">
        </activity>
        <activity android:name=".ChefLoginActivity">
        </activity>
        <activity android:name=".ShowMenuActivity">
        </activity>
        <activity android:name=".SignUpActivity">
        </activity>
        <activity android:name=".TimeActivity">
        </activity>
        <activity android:name=".TestActivity">
        </activity>
        <activity android:name=".TestDemoActivity">
        </activity>
        <activity android:name=".TimePickerActivity">
        </activity>

    </application>

</manifest>